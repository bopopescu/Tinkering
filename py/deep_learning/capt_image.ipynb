    "import cv2\n",
    "from time import sleep\n",
    "laps = 0.2\n",
    "nbframes = 60\n",
    "\n",
    "for i in range(nbframes):\n",
    "    sleep(laps)\n",
    "    cam = cv2.VideoCapture(0)\n",
    "    frame = cam.read()[1]\n",
    "    cv2.imwrite('img{0}.png'.format(i), frame)\n",
    "    sleep(laps)\n",
    "    #cv2.imshow(\"img1\", frame)"
   ]
